{
	"_version": "1.8.0",
	"sap.app": {
		"id": "alfaerp.template.Template",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"ServiceLayer": {
				"uri": "https://susesap:50000/b1s/v1/",
				"type": "OData",
				"settings": {
					"localUri": "model/metadata.xml"
				}
			},
			"apiOData": {
				"uri": "/cursoUI5/pagoti/helloEclipse/servico.xsodata/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "alfaerp.template.Template.view.HANA",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "alfaerp.template.Template.i18n.i18n"
				}
			},
			"": {
				"settings": {
					"defaultBindingMode": "TwoWay"
				},
				"dataSource": "ServiceLayer",
				"preload": true
			},
			"api": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "None",
					"disableHeadRequestForToken": true
				},
				"dataSource": "apiOData",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "alfaerp.template.Template.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl",
				"clearControlAggregation": false
			},
			"routes": [{
					"name": "HANA",
					"pattern": "HANA",
					"target": [
						"HANA"
					]
				}, {
					"name": "SL",
					"pattern": "",
					"target": [
						"SL"
					]
				}

			],
			"targets": {
				"HANA": {
					"viewName": "HANA"
				},
				"SL": {
					"viewName": "SL"
				}
			}
		}
	}
}